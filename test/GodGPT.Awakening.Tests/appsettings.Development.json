{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Orleans": "Debug",
      "Orleans.Runtime": "Information",
      "Orleans.Runtime.Management": "Warning",
      "Orleans.Runtime.Scheduler": "Warning",
      "GodGPT.Awakening": "Debug"
    }
  },
  "Awakening": {
    "EnableAwakening": true,
    "MaxRetryAttempts": 1,
    "TimeoutSeconds": 5,
    "LLMModel": "gpt-4o-mini",
    "Temperature": 0.5,
    "MaxTokens": 100,
    "EnableLanguageSpecificPrompt": true,
    "PromptTemplate": "Test prompt template: {CONTENT_SUMMARY} in {LANGUAGE}. Context: {USER_CONTEXT}. Date: {DATE}. Return JSON: {{\"level\": number, \"message\": \"string\"}}"
  },
  "Orleans": {
    "ClusterId": "awakening-dev-test",
    "ServiceId": "GodGPT.Awakening.Tests.Dev"
  }
}
